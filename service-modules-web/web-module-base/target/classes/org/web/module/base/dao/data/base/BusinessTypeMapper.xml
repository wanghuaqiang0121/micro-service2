<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.web.module.base.dao.data.base.BusinessTypeMapper">

	<!--查询业务类型列表 -->
	<select id="getList" parameterType="org.web.module.base.domain.data.base.BusinessType" resultType="map">
		SELECT
			id,
			`name`,
			`code`,
			create_date createDate
		FROM
			base_business_type
	</select>
</mapper>